  <DataSets>
    <DataSet Name="Dset_Sales_Orders">
      <Query>
        <DataSourceName>Dsrc_Local_Adventureworks</DataSourceName>
        <CommandText>SELECT	soh.* 
		, ter.CountryRegionCode
		, ter.Name AS Territory_Name
FROM	Sales.SalesOrderHeader soh
	LEFT JOIN
		Sales.SalesTerritory ter
			on soh.TerritoryID = ter.TerritoryID</CommandText>
      </Query>
